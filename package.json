    "eslint": "^8.0.1",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "postcss": "^8.4.14",
    "prettier": "^2.3.2",
